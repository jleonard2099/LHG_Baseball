' ...explanation...
Sub PLAYERNAME (P)

    If B7%(P, B1!(P)) = 1 Then
        player$(0) = pitchers$(P, P1%(P))
    Else
        player$(0) = batters$(P, B3%(P, B1!(P)))
    End If

    For pIdx = 1 To 3

        If gameB0%(pIdx, 0) > 0 Then
            If B7%(P, gameB0%(pIdx, 0)) <> 1 Then
                player$(pIdx) = batters$(P, B3%(P, gameB0%(pIdx, 0)))
