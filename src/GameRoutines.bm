'----------------------------------------
'       CREDITPUTOUT Subroutine
'----------------------------------------
'Credit player with infield putout
Sub CREDITPUTOUT (I3, FB2P%, AST%, D)

    If I3 > 4 Then I3 = 1

    '1=1st 2=2nd 3=3rd 4=home
    po% = I3 + 2

    If po% = 6 Then

        po% = 2
        AST% = 0
        I5 = B9%(D, po%)
        I6 = 16
        Call INCBATDF(D, I5, I6)

    Else

        If po% = 3 And FB2P% = 1 Then

            FB2P% = 0
            po% = 1
            If po% = 1 Then
                I5 = P1%(D)
                I6 = 38
                Call ADDPIT(D, I5, I6)
            End If

        Else

            If po% = 4 Then
                If AST% = 4 Then
                    po% = 6
                Else
                    If AST% = 6 Then
                        po% = 4
                    End If
                End If
            End If

        End If

        AST% = 0
        I5 = B9%(D, po%)
        I6 = 16
        Call INCBATDF(D, I5, I6)

    End If

End Sub

