                    Else
                        If P8%(I, I2, 0) = 0 Then
                            Print "--.-- ";
                        Else
                            If P8%(I, I2, 3) = 0 Then
                                Print " 0.00 ";
                            Else
                                Print Using "##.## "; P8%(I, I2, 3) / (P8%(I, I2, 0) / 3) * 9;
                            End If
